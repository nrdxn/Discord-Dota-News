{
    "compilerOptions": {
      "target": "ES2020",
      "outDir": "dist",
      "module": "CommonJS",
      "strict": true,
      "alwaysStrict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "skipLibCheck": true,
      "allowJs": true,
      "resolveJsonModule": true,
      "noImplicitAny": true,
      "paths": {
        "@/services/*": [
          "./src/core/services/*"
        ],
        "@/enums/*": [
          "./src/enums/*"
        ],
        "@/core/*": [
          "./src/core/*"
        ],
        "@/cfg/*": [
          "./src/configs/*"
        ],
        "@/app/*": [
          "./src/app/*"
        ]
      }
    }
  }